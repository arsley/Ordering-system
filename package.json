    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "autoprefixer": "^9.4.9",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^6.7.2",
    "file-loader": "^3.0.1",
